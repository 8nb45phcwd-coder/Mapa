{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@map/engine/*": ["engine/src/*"],
      "@map/ingestion/*": ["ingestion/src/*"],
      "@map/world_model/*": ["world_model/src/*"]
    }
  }
}
